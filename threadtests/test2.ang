[`time,`thread,`io] each {i library drop}
thread$mutex !Mutex



:f |a:|
    "go".
    []
    ?a 150 * dup 150+ range each {
        0.01 time$delay
        i 100.0 / sin,
        "." p io$stdout io$flush
    }
    ?Mutex thread$lock
    ?List + !List
    ?Mutex thread$unlock
;


(
    0!I 
    { 
        "Going round.. "p  ?I. !+I 
        [] !List [] 0 10 range each { i ?f thread$create,} thread$join
    }
)@

quit

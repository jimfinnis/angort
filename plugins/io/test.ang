"foo" "rb" open !F

0 !Lev
:showhash |:t|
    each {
        " " ?Lev * p
        i 20 padright p
        i iter get
        dup type !t
        ?t `list = if
            each {i p " " p} nl
        else
            ?t `hash = if nl
                ?Lev 1+ !Lev
                showhash
                ?Lev 1- !Lev
            else
                .
            then
        then
    }
;
?F readhash showhash


